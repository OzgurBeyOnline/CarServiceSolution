{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/CarService.Web/CarService.Web.csproj"],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "watch",
      "command": "dotnet",
      "type": "process",
      "args": ["watch", "run", "--project", "${workspaceFolder}/CarService.Web/CarService.Web.csproj"],
      "isBackground": true,
      "problemMatcher": "$msCompile"
    }
  ]
}
